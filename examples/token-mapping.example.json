{
  "color": {
    "match": [
      "primary",
      "secondary",
      "accent",
      "brand",
      "surface",
      "error",
      "warning",
      "info",
      "success",
      "gray"
    ],
    "rename": {
      "accent": "secondary",
      "brand": "primary",
      "neutral": "gray",
      "grey": "gray"
    }
  },
  "typography": {
    "strategy": "passthrough",
    "fontFamilyMapping": {
      "Google Sans": ["Inter", "system-ui", "-apple-system", "sans-serif"],
      "Roboto": ["Inter", "system-ui", "sans-serif"],
      "Space Mono": ["'Space Mono'", "monospace"]
    }
  },
  "spacing": {
    "baseUnit": 4,
    "scale": "linear"
  }
}
